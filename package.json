{
  "name": "pck.email.portfolio",
  "description": "Patrick Kilgore's programming portfolio/resume.",
  "author": "Patrick Kilgore <pckilgore@noreply.github.com>",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "test": "echo 'Write tests!!!!!!!!!!!!'",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "deploy": "nuxt generate && now --public && now alias",
    "dev": "nuxt",
    "start": "serve public",
    "dev-static": "serve public & nodemon --exec 'nuxt generate'",
    "generate": "nuxt generate",
    "precommit": "npm run lint"
  },
  "nodemonConfig": {
    "ignore": [
      "public/*"
    ],
    "ext": "*.vue"
  },
  "dependencies": {
    "nuxt": "^1.0.0",
    "vuetify": "^1.0.19"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "babel-plugin-transform-imports": "^1.4.1",
    "eslint": "^4.15.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.7.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-plugin-vue": "^4.0.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "husky": "^0.14.3",
    "nodemon": "^1.18.4"
  }
}
